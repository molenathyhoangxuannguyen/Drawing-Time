import turtle
wn = turtle.Screen()
wn.bgcolor("#E0FFFF")
mom = turtle.Turtle()
mom.shape("circle")
thy = int(input())
for comi in ["black", "white", "red", "lime", "blue", "yellow", "aqua", "magenta", "silver", "gray", "maroon", "olive", "green", "purple", "teal", "navy"]:
    mom.color(comi)
    for i in range(thy):
        mom.right(10)
        mom.forward(40)
        mom.stamp()
        mom.backward(thy)
        mom.dot()
        i +=1
wn.exitonclick()
